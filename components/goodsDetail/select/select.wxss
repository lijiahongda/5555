.content{
  width: 100%;
  height: auto;
  background: #fff;
  border-radius: 16rpx;
  /* margin-bottom: 20rpx; */
}
.list{
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #666666;
  font-size: 26rpx;
  padding: 0 22rpx;
  box-sizing: border-box;
}
.list .end{
  margin-left: auto;
  color: #ddd;
  display: flex;
  align-items: center;
}
.list .selected{
  color: #333;
  margin-left: 36rpx;
  width: 70%;
}
.list .end > text{
  padding-left: 12rpx
}
.list .couponList{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.list .couponList view{
  background: #FFEDEC;
  height: 44rpx;
  padding: 0 10rpx;
  font-size: 24rpx;
  color: #CA2519;
  text-align: center;
  line-height: 44rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
}

/* 优惠券列表 */
.couponGet{
  width:100%;
  height: 100%;
  background: rgba(0,0,0,.6);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1994;
}
.couponGet > view{
  width: 750rpx;
  height: 880rpx;
  background: #F6F6F6;
  position: absolute;
  left: 0;
  bottom: 0;
  padding: 30rpx 24rpx;
  padding-bottom: 150rpx;
  box-sizing: border-box;
}
.couponGet .couponGetList{
  width: 100%;
  height: 202rpx;
  background: #fff;
  border-radius: 18rpx;
  display: flex;
  margin-bottom: 20rpx;
}
.couponGetList > .left{
  width: 220rpx;
  height: 100%;
  background-image: url(https://yuetao-1300766538.cos.ap-beijing.myqcloud.com/yuetao/image/2020-01-16/14/yuelvhuizD47o9Z87Z1579157165.png);
  background-size: 100% 100%;
  color: #fff;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.couponGetList > .left > view:nth-child(1){
  font-size: 68rpx;
  line-height: 68rpx;
  padding-bottom: 10rpx;
  position: relative;
}
.couponGetList > .left > view:nth-child(1) > text:nth-child(1){
  font-size: 40rpx;
}
.couponGetList > .left > view:nth-child(2){
  font-size: 28rpx;
}
.couponGetList > .right{
  width: 482rpx;
  height: 100%;
  padding: 28rpx 20rpx 0 20rpx;
  box-sizing: border-box;
}
.couponGetList > .right > .title{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #141414;
  font-size: 28rpx;
  margin-bottom: 10rpx;
}
.couponGetList > .right > .title > view:nth-child(1){
  height: 40rpx;
  background: #F6AA07;
  font-size: 24rpx;
  color: #fff;
  border-radius:20rpx 20rpx 20rpx 0px;
  padding:  0 10rpx;
  box-sizing: border-box;
  margin-right: 10rpx;
}
.couponGetList > .right > .title > text:nth-child(2){
  width: 336rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.couponGetList > .right > view:nth-child(2){
  color: #999999;
  font-size: 24rpx;
  margin-bottom: 24rpx;
}
.couponGetList > .right > .get{
  background: #CA2519;
  height: 40rpx;
  border-radius:20rpx;
  color: #fff;
  font-size: 24rpx;
  width: 128rpx;
  line-height: 40rpx;
  text-align: center;
}
.couponGetList > .right > .overGet{
  background: #C4C4C4;
  height: 40rpx;
  border-radius:20rpx;
  color: #fff;
  font-size: 24rpx;
  width: 128rpx;
  line-height: 40rpx;
  text-align: center;
}
.couponGet .noMore{
  color: #999999;
  font-size: 26rpx;
  margin-top: 60rpx;
  text-align: center;
}
.couponGet .over{
  width: 100%;
  height: 150rpx;
  background: #F6F6F6;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  display: flex;
  align-items: center;
}
.couponGet .over view{
  width: 702rpx;
  height: 88rpx;
  background: #CA2519;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  border-radius: 44rpx;
  margin: 0 auto;
}
.listes{
  width: 100%;
