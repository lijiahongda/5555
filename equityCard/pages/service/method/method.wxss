/* equityCard/pages/service/method/method.wxss */
page{
    width:100%;
    height:100%;
    background: #f8f8f8;
}
.main{
  box-sizing: border-box;
  padding: 14rpx 27rpx 0 27rpx;
}
.box{
  background: #fff;
  box-sizing: border-box;
  padding: 27rpx;
}
.title{
  font-size: 28rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #333333;
  margin-bottom:26rpx;
}
.txts{
  font-size: 26rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  text-align: left;
  color: #666666;
  line-height: 40rpx;
}
.kefuBoxs{
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  margin-top:14rpx;
  background: #fff;
}
.kefu{
  line-height: 1;
  margin:0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
  padding: 0;
}
.kefu image{
  width:35rpx;
  height:35rpx;
  margin-right:14rpx;
}
.kefu  text{
  font-size: 34rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #333333;
}
.kefu::after{
  border:none;
}
