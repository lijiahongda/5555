/* equityCard/pages/Oiling/oiling.wxss */
page,.main{
  width: 100%;
  height: 100%;
  background: #f8f8f8;
}
.tishi{
  /* width:697rpx; */
  /* margin: 0 auto; */
  font-size: 25rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 500;
  /* text-align: left; */
  color: #999999;
}
.tabTitle {
  width:100%;
  line-height: 90rpx;
  border-radius: 13px;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
}
.tabTitle view {
  width:40%;
  font-size: 27rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #666666;
  display: flex;
  align-items: center;
  justify-content: center;

}
.tabTitle view image{
  width:19rpx;
  height:11rpx;
  margin-left:14rpx
}
.swiper-tab .swiper-tab-item.active{
  color: #f7263c;
}
.tabs{
  position:relative;
}
.tabBox{
  /* width: 100%;
  height:100vh;
  overflow: hidden;
  background: rgba(0,0,0,0.4);
  position: absolute;
  top:7vh; */
}
.bg,.bgs{
  width: 100%;
  height:100vh;
  overflow: hidden;
  background: rgba(0,0,0,0.4);
  position: absolute;
}
.bgs{
  background: rgba(0,0,0,0.6);
}
.box{
  width:100%;
  padding:28rpx 53rpx 33rpx 53rpx;
  background: #fff;
  box-sizing: border-box;
  border-top:1px solid  #f8f8f8;
  position: absolute;
  z-index:100
}
.kuai{
  margin-top: 33rpx;
}
.kuai .title{
  font-size: 27rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #333333;
}
.tabList>view{
  width: 150rpx;
  line-height: 57rpx;
  text-align: center;
  border: 1rpx solid #a8a8a8;
  border-radius: 13px;
  font-size: 27rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #999999;
  margin:20rpx 10rpx 0 0;
  display:inline-block;
}
.tabList>view:nth-child(4n){
  margin:20rpx 0 0 0
}
.tabList>view.active{
  color: #f7263c;
}
.noactice{
  color: #999999;
}
.hide{
  display: none;
 }
 .show{ 
  display: block;
}
.listBoxs{
  width:697rpx;
  margin:0 auto;
}
.list{
  background: #fff;
  margin:13rpx 0 0 0;
  box-sizing: border-box;
}
/* 列表 */
.list{
  box-sizing: border-box;
  padding: 30rpx 20rpx;
}
.imgBoxs{
  display: flex;
  box-sizing: border-box;
  padding-bottom: 30rpx;
}
.listImg{
  width:154rpx;
  height:154rpx;
  border-radius: 10rpx;
  margin-right:30rpx
}
.txtBoxs {
  width: 470rpx;
}
.txtBoxs .title{
  font-size: 32rpx;
  font-family: PingFang SC Medium, PingFang SC Medium-Medium;
  font-weight: 500;
  color: #333333;
}
.priceTxt{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin:8rpx 0
}
.priceTxt .price{ 
  font-size: 44rpx;
  font-family: PingFang SC Semibold, PingFang SC Semibold-Semibold;
  font-weight: 600;
  color: #f7263c;
}
.priceTxt .kms{
  font-size: 24rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #666666;
}
.priceLine{
  display: flex;
  align-items: center;
}
.priceLineImgs image{
  width:13rpx;
  height:15rpx;
}
.priceLineImgs text{
  font-size: 21rpx;
  font-family: PingFang SC Medium, PingFang SC Medium-Medium;
  font-weight: 500;
  color: #f7263c;
  padding-left: 6rpx;
}
.priceLineImgs>view {
  width:30rpx;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  background: #f7263c;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}
.priceLineImgs {
  width: 138rpx;
  height: 35rpx;
  line-height: 35rpx;
  border: 1px solid #f7263c;
  border-radius: 7px;
  display: flex;
  align-items: center;
}
.priceLine .txt{
  font-size: 23rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  text-decoration:  line-through;
  color: #999999;
  margin-left:27rpx;
}
.listBottom{
  border-top:1px solid #eeeeee;
  padding-top:22rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.leftImg {
  display: flex;
  align-items: center;
}
.leftImg image{ 
  width: 20rpx;
  height: 27rpx;
}
.leftImg text{
  width:420rpx;
  font-size: 24rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #666666;
  margin-left:8rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.maps{
  width: 170rpx;
  height:56rpx;
}
.bottom{
  width: 100%;
  height:149rpx;
  box-sizing: border-box;
  padding-top:14rpx;
  background-color:  #FB376D;
  background-image: linear-gradient(to right, #FB376D , #FE1A38,#F71029);
  position: fixed;
  bottom:0
}
.bottom .txt{
    width: 697rpx;
    line-height: 66rpx;
    background: #fff;
    border-radius: 33rpx;
    margin:0 auto;
    font-size: 29rpx;
    font-family: PingFang SC Regular, PingFang SC Regular-Regular;
    font-weight: 400;
    text-align: center;
    color: #f7263c;
}
/* 下拉 */
.xiala {
  width: 100%;
  text-align: center;
  /* display: flex;
  align-items: center;
  justify-content: center */
  position: absolute;
  bottom:-8vh
}
.xiala text{
  font-size: 22rpx;color: #999;
  margin-right:20rpx;
}
.xiala image{
  width: 20rpx;height: 20rpx;
  animation: heart 1s ease-in-out 2s infinite alternate;
}
/* 弹窗 */
.modalBoxs{
  position: fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
}
.modalX{
  width:33rpx;
  height:33rpx;
  position: absolute;
  right:27rpx;
  top:27rpx
}
.modalContent{
  width: 539rpx;
  height: 445rpx;
  background: #ffffff;
  border-radius: 13rpx;
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  left:0;
  right:0;
  margin:0 auto;
  z-index:100;
  text-align: center;
}
.modalError{
  width:95rpx;
  height:95rpx;
  text-align: center;
  margin: 32rpx auto;
}
.modaError-txt{
  font-size: 27rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  color: #f7263c;
  margin-bottom:25rpx
}
.modals .txt{
  font-size: 24rpx;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
}

.logoSmall{
  width:63rpx;
  height:63rpx;
  border-radius: 50%;
  margin-right:20rpx
}
.modalLogo{
  margin: 20rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.modalBtns{
  width:100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.modalBtns view{
  width:50%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 24rpx;
  font-family: PingFang SC Medium, PingFang SC Medium-Medium;
  font-weight: 500;
  text-align: left;
  color: #e9e9ea;
  text-align: center;
}
.modalBtns view:first-child{
  background: #f4f4f4;
 color: #333333;
}
.modalBtns view:last-child{
  background: red;
  color: #e9e9ea;
}
.headTop{
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 25rpx 20rpx;
}
.headTop .kefu{
  font-size: 26rpx;
  font-weight: 500;
  color: #333;
}