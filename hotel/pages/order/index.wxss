/* 订房必读 */
.read_box{
    width: 100%;
    padding: 40rpx 20rpx 40rpx 32rpx;
    box-sizing: border-box;
    position: fixed;
    left: 0;
    bottom: 0;
    background: #fff;
    border-radius:14rpx 14rpx 0px 0px;
    z-index: 999;
    min-height: 900rpx;
    font-weight: 600;
    max-height: 1100rpx;
}
.read_box .title{
    font-size: 34rpx;
    color: #333333;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 70rpx;
}
.read_box .title image{
    width: 34rpx;
    height: 34rpx;
    margin-right: 20rpx;
}
.read_box .close{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
}
.read_box .list{
    display: flex;
    align-items: flex-start;
    margin-bottom: 40rpx;
    font-weight: 400;
    line-height: 40rpx;
    font-size: 24rpx;
}
.read_box .list .list_le{
    font-size: 40rpx;
}
.read_box .list .list_ri .titles{
    font-size: 28rpx;
    color: #333;
    height: 40rpx;
    font-weight: 600;
}
/* 联名卡弹框 */
.read_box .card_title{
    display: block;
    text-align: center;
    margin-bottom: 50rpx;
}
.read_box .card_title view:first-child{
    font-size: 40rpx;
    color: #333333;
    font-weight: 600;
    margin-bottom: 6rpx;
}
.read_box .card_title view:last-child{
    font-size: 20rpx;
    color: #666;
    font-weight: 300;
}
.read_box .card_list{
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 750rpx;
    margin-left: -30rpx;
}
.read_box .card_list .card_cont{
    width: 25%;
    text-align: center;
    font-size: 20rpx;
    color: #999;
    font-weight: 400;
    margin-bottom: 40rpx;
}
.read_box .card_cont image{
    width:92rpx;
    height:92rpx;
    margin-bottom: 6rpx;
    border-radius: 50%;
}
.read_box .card_cont .tit{
    font-size: 24rpx;
    color: #333;
    font-weight: 500;
}
.read_box .card_notice{
    font-size: 24rpx;
    color: #666666;
    line-height: 40rpx;
    font-weight: 400;
}
.read_box .card_notice view{
    margin-bottom: 10rpx;
}
.read_box .card_notice view.notice_title{
    font-size: 34rpx;
    color: #333333;
    font-weight: 600;
}
.read_box .card_btn{
    width:670rpx;
    height:72rpx;
    line-height: 72rpx;
    background:linear-gradient(261deg,rgba(248,38,59,1),rgba(255,79,64,1));
    border-radius:36rpx;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    margin: 74rpx auto;
}



/* 优惠券弹框 */
.coupon_list{
    width:670rpx;
    height:180rpx;
    background:url(http://image.zhiding365.com/2020/8/11/28d68cad-76e4-4837-a76a-9eedda1aebe7.png);
    background-size: 670rpx 180rpx;
    border-radius:14rpx;
    display: flex;
    margin: 0 auto;
    margin-bottom: 26rpx;
    position: relative;
}
.coupon_list .coupon_type{
    position: absolute;
    top: 0;
    left: 0;
    padding: 0 24rpx;
    height:40rpx;
    line-height: 40rpx;
    background:rgba(247,38,60,1);
    border-radius:12rpx 0px 12rpx 0px;
    font-size: 22rpx;
    color: #fff;
}
.coupon_list .list_le{
    padding-top: 20rpx;
    box-sizing: border-box;
    width:480rpx;
    text-align: center;
    font-size: 20rpx;
    color: #666;
    line-height: 34rpx;
    font-weight: 400;
}
.coupon_list .list_le .coupon_title{
    font-size: 32rpx;
    color: #333333;
    margin-bottom: 10rpx;
    margin-top: 20rpx;
    font-weight: 500;
}
.coupon_list .list_ri{
    width:190rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 30rpx;
    color: #333;
    font-weight: 400;
}
.coupon_list .list_ri view{
    font-size: 52rpx;
    color: #333333;
    font-weight: 600rpx;
}
.coupon_list .coupon_act{
    position: absolute;
    right: 0;
    top:0;
    width: 72rpx;
    height: 72rpx;
}