}
.ofooter .ofooter_le{
    width: 548rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.ofooter .shareGain {
    width: 204rpx;
    font-size: 28rpx;
    background-color: #282728;
    color: #fff;
    height: 114rpx;
    line-height: 114rpx;
}
.ofooter .shareGain .contact{
    width: 204rpx;
    height: 114rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    font-weight: 500;
    margin: 0;
    padding: 0;
    line-height: 114rpx;
}

.ofooter .btnpay {
    width: 204rpx;
    background-color: #F7263C;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
    line-height: 114rpx;
}


.contentList {
    width: 696rpx;
    position: relative;
    margin: 0 auto;
    margin-top: -360rpx;
}


.pop {
    position: fixed;
    bottom: 0;
    z-index: 101;
    left: 0;
}

.maincontent {
    position: absolute;
    bottom: 98rpx;
    background-color: #fff;
