page{
    background: #F8F8F8;
}
.HeadContent {
    height: 366rpx;
    width: 100%;
    background-size: 100%;
    background-image: url(https://yuetao-1300766538.cos.ap-beijing.myqcloud.com/yuetao/image/2020-04-15/22/yuelvhuiNb2pJTqArR1586959965.png);
    display: flex;
    flex-direction: column;
    align-items: center;
}
/* 顶部信息 */
.oheader {
    width: 696rpx;
    box-sizing: border-box;
    background-color: #fff;
    /* box-shadow: 0 0 10rpx 0 rgba(72, 84, 92, .08); */
    border-radius: 12rpx;
    padding: 20rpx 22rpx;
    margin-bottom: 20rpx;
}
.odate {
    display: flex;
    font-size: 30rpx;
    color: #333;
    align-items: center;
    font-weight: 500;
}
.dnum {
    padding: 0 12rpx;
    border: 1rpx solid #666666;
    height: 24rpx;
    line-height: 24rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    justify-content: center;
    font-size: 20rpx;
    color: #666;
    margin: 0 12rpx;
}
.roominfo {
    color: #333333;
    font-size: 30rpx;
    font-weight: 500;
    margin-top: 16rpx;
    padding-bottom: 22rpx;
    border-bottom: 1rpx solid rgba(238,238,238,1);
    margin-bottom: 22rpx;
}
.detal_list{
    display: flex;
    align-items: center;
    line-height: 25rpx;
    font-size: 24rpx;
    color: #666666;
    margin-top: 20rpx;
    font-weight: 400;
}
.detal_list view{
    border-right: 2rpx solid #EEEEEE;
    padding: 0 18rpx;  
}
.detal_list view:first-child{
    padding-left: 0;  
}
.detal_list view:last-child{
    border: none;
}
.roombot .bot_list{
    font-size: 24rpx;
    color: #666666;
    display: flex;
    align-items: center;
}
.roombot .bot_list image{
   width: 20rpx;
   height: 20rpx;
   margin-right: 12rpx;
}

/* 订房必读 */
.reading{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.reading .title{
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #333;
    font-weight: 600rpx;
    margin-bottom: 10rpx;
}
.reading .title image{
    width: 26rpx;
    height: 26rpx;
    margin-right: 12rpx;
}
.reading .more{
    font-size: 22rpx;
    color: #F7263C;
}
.read_list{
    font-size: 24rpx;
    color: #333;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    width: 100%;
    line-height: 40rpx;
    display: flex;
    align-items: center;
}
.read_list text:first-child{
    font-size: 50rpx;
}


/* 入住信息 */
.olist {
    padding: 0;
}
.olist .item {
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #666666;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 22rpx;
}
.olist.item_title{
    font-size: 24rpx;
    color: #666666;
    padding: 0 22rpx;
}
.item_title .title{
    font-size: 33rpx;
    color: #333333;
    font-weight: 600;
    margin: 10rpx 0;
}
.olist .item:last-child {
    border-bottom: 0 solid #000;
}
.olist .item_le{
    display: flex;
    align-items: center;
    justify-content: flex-start;
}
.olist .item_le view{
    color: #333;
    font-size: 28rpx;
}
.olist .item_le view:nth-child(2n){
    font-weight: 500;
    line-height: 40rpx;
}
.olist .item_le .phone{
    font-size: 28rpx;
    color: #333333;
    display: flex;
    align-items: center;
    font-weight: 500;
}
.olist .item_le .phone text{
    margin-right: 14rpx;
    padding-right: 14rpx;
    border-right: 1rpx solid #EEEEEE;
}
.item_title .item_le{
    font-size: 24rpx;
    color: #666666;
}
.olist .item_ri{
    width: 20rpx;
    height: 12rpx;
}
.label-text {
    width: 170rpx;
    font-size: 28rpx;
    color: #666666;
}
/* 房间数 */
.olist .rooms{
    background: #F8F8F8;
    display: flex;
    align-items:center;
    /* justify-content: space-between; */
    padding-top: 30rpx;
    flex-wrap: wrap;
}
.olist .rooms view{
    width:116rpx;
    height:60rpx;
    background:rgba(255,255,255,1);
    border-radius:30rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 24rpx;
    color: #333333;
    margin-right: 24rpx;
    margin-bottom: 26rpx;
}
.olist .rooms view.act{
    border:1rpx solid rgba(255,0,0,1);
}
.olist .rooms view:nth-child(5n){
    margin-right: 0;
}
/* .olist  */

/* 本单可享 */
.olist .item_le .discount{
    font-size: 28rpx;
    color: #F7263C;
    display: flex;
    align-items: center;
}
.olist .item_le .discount view{
    font-size: 28rpx;
    color: #F7263C;
    display: flex;
    align-items: center;
    font-weight: 400;
}
.olist .item_le .discount .btn{
    padding: 0 6rpx;
    height:30rpx;
    line-height: 30rpx;
    border:1rpx solid rgba(247,38,60,1);
    border-radius:4rpx;
    font-size: 20rpx;
    color: #F7263C;
    margin-left: 6rpx;
    font-weight: 400;
}


/* 联名卡 */
.card{
    position: relative;
}
.card .top_le{
    position: absolute;
    left: 0;
    top: 0;
    width:158rpx;
    height:38rpx;
    line-height: 38rpx;
    border-radius:12rpx 0 12rpx 0;
    background:linear-gradient(103deg,rgba(242,210,139,1) 0%,rgba(225,181,108,1) 100%);
    display: flex;
    align-items: center;
}
.card .top_le text{
    font-size: 24rpx;
    font-family:PingFang SC;
    font-weight:600;
    color:rgba(51,51,51,1);
    background:linear-gradient(-33deg,rgba(80,72,73,1) 0%, rgba(113,113,113,1) 40.087890625%, rgba(33,34,33,1) 100%);
    -webkit-background-clip:text;
    -webkit-text-fill-color:transparent;
}
.card .top_le image{
    width: 18rpx;
    height: 24rpx;
    margin-top: 5rpx;
    margin-left: 8rpx;
    margin-right: 8rpx;
}
.card .card_cont{
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #333333;
    margin-top: 38rpx;
    font-weight: 600;
}
.card .card_cont .title{
    font-size: 24rpx;
    color: #999999;
}
.card .card_cont .btn{
    width:180rpx;
    height:64rpx;
    background:linear-gradient(261deg,rgba(248,38,59,1),rgba(255,79,64,1));
    border-radius:32rpx;
    font-size: 20rpx;
    color: #FFFFFF;
    text-align: center;
    font-weight: 400;
}
.olist .item_bot{
    border: none;
    padding: 10rpx 22rpx;
}
.olist .item_bot .label-text{
    width: 50%;
}




.cb-icon {
    width: 26rpx;
    height: 26rpx;
    border: 1rpx solid #f3f3f3;
    box-sizing: border-box;
    border-radius: 4rpx;
    background-color: #22bf43;
    border: 0;
}

.cb-icon:after {
    color: #fff;
    display: block;
    text-align: center;
    line-height: 26rpx;
    content: "\221A";
    font-size: 20rpx;
}

.ofooter {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 750rpx;
    height: 114rpx;
    font-size: 22rpx;
    display: flex;
    background-color: #fff;
    z-index: 101;
    box-shadow:0px -3px 27px 0px rgba(153,153,153,0.2);
    box-sizing: border-box;
    align-items: center;
    justify-content: space-between;
}

.ofooter .gprice {
    display: flex;
    align-items: center;
    width: 230rpx;
    padding-left: 24rpx;
}

.gprice .price {
