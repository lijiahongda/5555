    justify-content: center;
}

.couponlist {
    font-size: 30rpx;
    color: #666;
    height: 500rpx;
    overflow-y: scroll;
}

.cou-list {
    display: flex;
    padding: 0 30rpx;
    height: 115rpx;
    -ms-flex-pack: justify;
