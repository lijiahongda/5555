.pronum {
    display: flex;
    float: right;

}

.pronumcon {
    /* display: flex;
    justify-content: flex-end;
    padding-bottom: 50rpx; */
    padding:0 30rpx;
    box-sizing: border-box;
    margin-bottom: 200rpx;
}
.pronumcon .name{
    float: left;
    font-size: 28rpx;
    color: #141414;
}

.label {
    height: 56rpx;
    line-height: 56rpx;
    padding: 0 37rpx;
    color: #333333;
    font-size: 28rpx;
    background-color: #F6F6F6;
    margin: 0 2rpx;
}

.tab-box {
    max-height: 400rpx;
    overflow-y: auto;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.ul-li-active {
    background-color: #ffeded;
    color: #CA2519;
    border: 1px solid #E33F44;
    box-sizing: border-box;
}