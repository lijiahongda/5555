page {
  background: #f8f8f8;
  font-family: Microsoft YaHei Regular;
  width: 100%;
  overflow-x: hidden;
  overflow-y: visible;
}
.home_top{
  background:#5E4834;
  padding-bottom: 21rpx;
}
.bannerimage{
  width: 750rpx;
  height: 558rpx;
}
.list_title{
  height: 97rpx;
  line-height: 97rpx;
  color:#333333;
  font-weight: 500;
  font-size: 30rpx;
  text-align: center;
}
.listContent {
  background-image: url('http://image.zhiding365.com/2020/9/7/b144e3c4-33ae-4d72-9633-6ab16da72f11.png');
  background-size:100% 100%;
  width: 722rpx;
  height: 731rpx;
  margin: 0 auto;
  border-radius: 18rpx;
  margin-bottom: 9rpx;
}

.Location {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #DEDEDE;
  height: 100rpx;
  padding: 0 31rpx;
}

.Location .loc {
  /* border-right: 1px solid #999; */
  width: 19.2%;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(36, 36, 36, 1);
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  margin-right: 3.3%;
  padding-right: 2%;
}

.Location .loc text {
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}

.Location .loc image {
  margin-left: 5rpx;
  width: 10rpx;
  height: 10rpx;
  margin-bottom: 9rpx;
}

.Location .address {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  width: 63%;
}

.Location .address image {
  width: 18rpx;
  height: 18rpx;
}

.Location .CurrentPositioning {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 18rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #3886EE;
}

.Location .CurrentPositioning image {
  width: 34rpx;
  height: 34rpx;
  margin-bottom: 8rpx;
  font-size: 18rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.timeSelect {
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #DEDEDE;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(36, 36, 36, 1);
  padding: 0 31rpx;
}

.timeSelect .CheckTime {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 27%;
}

.timeSelect .CheckTime .timeTips {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-left: 1.4%;
  margin-top: 1%;
}

.timeSelect .Connector {
  width: 42rpx;
  height: 2rpx;
  background: rgba(153, 153, 153, 1);
  margin-right: 3%;
}

.timeSelect .SeveralNights {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(36, 36, 36, 1);
  width: 36%;
}

.timeSelect .SeveralNights image {
  width: 14rpx;
  height: 20rpx;
  margin-left: 8rpx;
}

.screen {
  height: 86rpx;
  border-bottom: 1px solid #DEDEDE;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
}
.screenN{
  height: 100rpx;
  border-bottom: 1px solid #DEDEDE;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 31rpx;
}
.screenN image{
  height:18rpx ; 
  width: 18rpx;
}

.PriceSetting {
  width: 662rpx;
  height: 88rpx;
  background: linear-gradient(-70deg, #D89679, #E4B39D);
  border-radius: 44rpx;
  margin:26rpx auto;
  font-size: 33rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 88rpx;
}
.list_bottom{
  display: flex;
align-items: center;
margin-top: 54rpx;
}
.bottom_left image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.bottom_left{
  display: flex;
align-items: center;
font-size: 24rpx;
color: #666666;
margin-left: 32rpx;
}
.home_icon{
  height: 176rpx;
background: #fff;
width: 722rpx;
border-radius: 12rpx;
margin: 0 auto;
display: flex;
justify-content: space-around;
}
.icon_list image{
  width: 60rpx;
  height: 60rpx;
}
.icon_list{
  text-align: center;
margin-top: 33rpx;
font-size: 28rpx;
color: #333333;
position: relative;

}
.vipCla{
  width: 80rpx;
height: 24rpx;
background: linear-gradient(-43deg, #E64E52, #F92F47);
border-radius: 12rpx 12rpx 12rpx 0rpx;
font-size: 18rpx;
font-family: PingFang SC;
font-weight: 400;
color: #FFFFFF;
position: absolute;
right: -30rpx;
top: -10rpx;

}
.selfLife_icon{
  height: 230rpx;
background: #fff;
width: 722rpx;
border-radius: 12rpx;
margin: 0 auto;
display: flex;
justify-content: space-around;
}

.selfLife_tit{
display: flex;
justify-content: space-between;
margin: 40rpx 24rpx 30rpx 19rpx;

}
.selfLife_text{
font-size: 24rpx;
font-family: PingFang SC;
font-weight: 400;
color: #666666;
}
.selfLife_content{
font-size: 22rpx;
font-family: PingFang SC;
font-weight: 400;
color: #999999;
}