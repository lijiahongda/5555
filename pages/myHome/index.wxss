.indexTip {
  position: fixed;
  right: 42rpx;
  z-index: 9;
}
.indexTip .tip {
  width: 440rpx;
  border-radius: 6rpx;
  background: linear-gradient(to right, #f55f64, #f99374);
  padding: 15rpx 22rpx;
  position: relative;
  color:#fff;
  display: flex;
align-items: center;
justify-content: space-between;

}
.indexTip .tip::before {
  content: "";
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-bottom: 17rpx solid #f99374;
  position: absolute;
  top: -14rpx;
  right: 95rpx;
}
.indexTip .text {
  font-size: 22rpx;
}
.indexTip .text image {
  width: 30rpx;
  height: 16rpx;
  display: inline-block;
}
.indexTip .iconfont {
  color: #cdcdcd;
  font-size: 32rpx;
}