
page {
  background-color: #fff;
}

.icon {
  width: 100%;
  margin-top: 61rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-bottom: 60rpx;
}

.icon image {
  width: 76rpx;
  height: 76rpx;
}

.title {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 29rpx;
}

.tips {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 45rpx;
}

.list {
  border: 1px solid #a2a2a2;
  width: 83%;
  margin-left: 8.5%;
  margin-right: 8.5%;
  margin-bottom: 60rpx;
}

.list .item {
  height: 80rpx;
  padding-left: 5%;
  padding-right: 5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid rgba(162, 162, 162, 1);
}

.list .item .info {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.list .item .info .num {
  margin-right: 50rpx;
}

.list .item image {
  width: 28rpx;
  height: 28rpx;
}

.Submission {
  width: 83%;
  margin-left: 8.5%;
  margin-right: 8.5%;
  height: 88rpx;
  background-color: #ccc;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-bottom: 100rpx;
}
