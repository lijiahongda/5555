/* page/community/pages/main/group/activation/components/gshare/index.wxss */
.shareRecord{
  width: 100%;
  height: 254rpx;
  background: #fff;
  margin-top: 20rpx;
  overflow: hidden;
}
.shareRecord>.title{
  width: 702rpx;
  line-height: 50rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  display:flex;
  justify-content: space-between;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}

.shareRecord>.title>.name{
  color:rgba(20,20,20,1);
}

.shareRecord>.title>.Range{
  display: flex;
  line-height: 50rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
}

.shareRecord>.title>.Range>.text{
  padding:0 15rpx;
}

.shareRecord>.title>.Range .datemsg{
  display: flex;
  width: 181rpx;
  height: 44rpx;
  border:2rpx solid rgba(221,221,221,1);
  border-radius:8rpx;
  line-height: 44rpx;
  
}


.shareRecord>.title>.Range .datemsg>view>image{
  width: 40rpx;
  height: 40rpx;
}

.shareRecord>.title>.Range .datemsg>.ymddate{
  width: 140rpx;
  height: 40rpx;
  text-align: center;
  font-size:24rpx;
  line-height: 40rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  padding-left: 10rpx;
}

.peopleNum{
  width: 550rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  margin-top: 42rpx;
}

.peopleNum>view{
  width: 140rpx;
  text-align: center;
}

.peopleNum>view>.num{
  font-size:50rpx;
  line-height: 50rpx;
  font-family:PingFang SC;
  font-weight:600;
  color:rgba(20,20,20,1);
}

.peopleNum>view>.name{
  font-size:24rpx;
  font-family:PingFang SC;
  line-height: 24rpx;
  font-weight:500;
  margin-top: 25rpx;
  color:rgba(153,153,153,1);
}

.shareList{
  width:702rpx;  
  min-height:500rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  margin: 20rpx auto;
  overflow: hidden;
  padding-bottom: 30rpx;
}

.shareList>.date{
  width: 662rpx;
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  margin-top: 30rpx;
  line-height: 40rpx;
}

.shareList>.date>.ymd{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
}

.shareList>.date>.endDate{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
}

.shareList>.date>.endDate>.num{
  border-radius:8rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  background: #000;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}

.shareList>.date>.endDate>.fh{
  padding: 0 10rpx;
}

.shareList>.shareComm{
  width: 662rpx;
  height: 240rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  margin-top: 28rpx;
}

.shareList>.shareComm>.cover{
  width: 240rpx;
  height: 240rpx;
  background:rgba(180,180,180,1);
  border-radius:16rpx;
  overflow: hidden;
}

.shareList>.shareComm>.cover>image{
  width: 100%;
  height: 100%;
}

.shareList>.shareComm>.msg{
  width: 391rpx;
  height: 240rpx;
}

.shareList>.shareComm>.msg>.title{
  width: 100%;
  height: 66rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height: 33rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-top: 13rpx;
}

.shareList>.shareComm>.msg>.price{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
  margin-top: 30rpx;
}

.shareList>.shareComm>.msg>.price>text{
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(202,37,25,1);
}

.shareList>.shareComm>.msg>.getMoney{
  height: 48rpx;
  display: flex;
  font-size:24rpx;
  line-height: 48rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
  margin-top: 20rpx;
} 

.shareList>.shareComm>.msg>.getMoney>.gmemoney{
  width: 139rpx;
  height: 48rpx;
  display: flex;
  background:rgba(249,226,174,1);
  border-radius:8rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(26,23,21,1);
  line-height: 48rpx;
}

.shareList>.shareComm>.msg>.getMoney>.gmemoney>.zhuan{
  width: 48rpx;
  height: 48rpx;
  margin-right: 5rpx;
}
.shareList>.shareComm>.msg>.getMoney>.gmemoney>.zhuan>image{
  width: 100%;
  height: 100%;
}

.groupMoney{
  margin-left: 10rpx;
}

.groupMoney>text{
  color: #CA2519;
}

.user{
  /* width: 100%; */
  height: 80rpx;
  display: flex;
  justify-content:space-between;
  margin: 0 20rpx;
  margin-top: 30rpx;
}

.user>.cover{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-left: 10rpx;
}

.user>.cover>image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  overflow: hidden;
}

.user>.msg{
  width: 400rpx;
  height: 80rpx;
}

.user>.msg>.name{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.user>.msg>.seeNum{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.user>.copyBt{
  width: 132rpx;
  height: 58rpx;
  /* background:rgba(187,187,187,1); */
  border-radius:8rpx;
  text-align: center;
  line-height: 58rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  border:1px solid rgba(221,221,221,1);
  margin-top: 10rpx;
}

.seeMore{
  width: 140rpx;
  height: 40rpx;
  line-height:40rpx;
  margin: 0 auto;
  margin-top: 29rpx;
  display: flex;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}

.seeMore image{
  width: 40rpx;
  height: 40rpx;
}

.alert{
  width:100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5)
}

.codeAlert{
  width: 600rpx;
  height: 600rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 10;
}
.codeAlert>.text{
  width: 100%;
  font-size:32rpx;
  text-align: center;
  line-height: 32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(20,20,20,1);
  margin-top: 60rpx;
  margin-bottom: 60rpx;
}
.codeAlert>.contentText{
  /* width: 520rpx; */
  height: 600rpx;
  margin: 0 auto;
  padding-bottom: 30rpx;
  overflow-x: hidden;
}
.codeAlert>.contentText>textarea{
  width: 100%;
  height: 100%;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(20,20,20,1);
}
.codeAlert>.close{
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  top: 110%;
  transform: translate(-50%,-50%)
}
.codeAlert>.close>image{
  width:100%;
  height: 100%;
}
.codeAlert>.inputCode{
  width: 518rpx;
  height: 80rpx;
  border-bottom: 2rpx solid rgba(221,221,221,1);
  margin: 0 auto;
  box-sizing: border-box;
  margin-bottom:20rpx;
}
.codeAlert>.inputCode>input{
  text-align: center;
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(20,20,20,1);
}
.codeAlert>.sText{
  text-align: center;
  line-height: 24rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-bottom: 92rpx;
  margin-top: 60rpx;
}
.codeAlert>.alertBt{
  width:518rpx;
  height:86rpx;
  background:rgba(202,37,25,1);
  text-align: center;
  line-height: 86rpx;
  border-radius:16rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin: 0 auto;
  margin-top: 60rpx;
}
.none{
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #999;
  margin-top: 40rpx;
}