/* 分享层 */

.share-layer {
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10001;
}

.share-layer-mask {
  width: 100%;
  height: 100%;
}

.share-layer-block {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 430rpx;
  background: #fff;
  text-align: center;
  z-index: 2;
  border-radius:18rpx 18rpx 0rpx 0rpx;
  justify-content: space-between;
  display: flex;
}

.share-layer-block-img {
  width: 100%;
  height: 75%;
  float: left;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
}

.share-layer-block image {
  margin-top: 21rpx;
  width: 100rpx;
  height: 108rpx;
  /* margin-bottom: 21rpx; */
}

.share-layer-block-img button {
  height: 40%;
  background: #fff;
  border: 0;
  margin: 0;
  padding: 0;
  border-color: #fff;
  box-shadow: 0;
}

.share-layer-block-img button::after {
  border: none;
}

.wxParse-img {
  width: 100%;
  display: block;
}
.poster{
  position: fixed;
  top: 5%;
  left: 0;
  width: 100%;
  height: 90%;
}