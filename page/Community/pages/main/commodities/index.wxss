}

.ugc{
  width: 650rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.ugc>.ugc_user{
  width: 100%;
  height: 80rpx;
  display: flex;

}

.ugc>.ugc_user>.uu_cover{
  width: 80rpx;
  height: 80rpx;
  border-radius:50%;
  overflow: hidden;
  margin-right: 10rpx;
}

.ugc>.ugc_user>.uu_cover>image{
  width: 100%;
  height: 100%;
  border-radius:50%;
  overflow: hidden;
}

.ugc>.ugc_user>.uu_msg{
  width: 540rpx;
  height: 100%;
}

.ugc>.ugc_user>.uu_msg>.name{
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:40rpx;
}

.ugc>.ugc_user>.uu_msg>.brief{
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:40rpx;
}

.ugc>.ugc_content{
  width: 100%;
  max-height: 150rpx;
  margin: 30rpx auto;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  line-clamp: 4;
  -webkit-box-orient: vertical;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:38rpx;
}

.ugc>.ugc_img{
  width: 100%;
  display: flex;
  clear: both;
}

.ugc>.ugc_img>.img_one{
  width: 400rpx;
  height: 400rpx;
  border-radius:16rpx;

}

.ugc>.ugc_img>.img_one>image{
  width: 100%;
  height: 100%;
  border-radius:16rpx;
}

.ugc>.ugc_img>.img_two{
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.ugc>.ugc_img>.img_two>view{
  width: 310rpx;
  height: 310rpx;
  border-radius:16rpx;
}

.ugc>.ugc_img>.img_two>view>image{
  width: 100%;
  height: 100%;
  border-radius:16rpx;
}

.ugc>.ugc_img>.img_more{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  clear: both;
}

.ugc>.ugc_img>.img_more>view{
  width: 200rpx;
  height: 200rpx;
  border-radius:16rpx;
  float: left;
  margin-bottom: 20rpx;
}

.ugc>.ugc_img>.img_more>view>image{
  width: 100%;
  height: 100%;
  border-radius:16rpx;
}
