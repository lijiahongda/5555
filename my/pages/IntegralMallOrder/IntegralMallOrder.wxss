.tab-h {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 80rpx;
  background: #fff;
  font-size: 30rpx;
  white-space: nowrap;
  z-index: 99;
  box-shadow: 0 4rpx 8px #ddd;
}

.noorder {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 900rpx;
}

.noorder image {
  width: 255rpx;
  height: 275rpx;
}

.noorder .go {
  width: 274rpx;
  height: 68rpx;
  background: rgba(242, 99, 86, 1);
  border-radius: 34rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 68rpx;
  text-align: center;
  margin-top: 103rpx;
}

.malllogo {
  width: 40rpx;
  height: 40rpx;
}

.tab-item {
  margin: 0 3.3%;
  width: 13%;
  display: inline-block;
  text-align: center;
  position: relative;
}

.tab-item.active {
  color: #ff6032;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 4rpx;
  width: 100%;
  background: #ff6032;
  position: absolute;
  bottom: 0;
}

.tab-con image {
  width: 100%;
  height: 100%;
}

.tab-con {
  /* padding: 25rpx 0; */
  min-height: 100%;
  position: relative;
}

.tab-con.gwc {
  padding-bottom: 125rpx;
}

.scoll-h {
  height: 100%;
  position: relative;
}

.list {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-top: 20rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.list .title {
  display: flex;
  flex-direction: row;
  width: 93%;
  margin: auto;
  padding-bottom: 20rpx;
  margin-left: 4%;
  /* justify-content: space-between  */
}

.list .title .image {
  border-left: 1px solid #ececec;
  padding-left: 10%;
  margin-left: 10%;
  height: 41rpx;
  display: flex;
  align-items: center;
  width: 30%;
}

.list .title .image image {
  width: 30rpx;
  height: 30rpx;
}

.list .title .state {
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-between;
}

.ListContent {
  /* display: flex; *//* width: 93%; *//* margin: auto; *//* height: 200rpx; */
  align-items: center;
  padding-bottom: 50rpx;
  /* flex-direction: row; */
  padding-top: 20rpx;
}

.borderBottom {
  border-top: 1px solid #ececec;
}

.tobe_aid .image, .complete .cont {
  width: 100%;
  display: block;
  background: rgba(244, 244, 244, 1);
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.complete .cont {
  display: flex;
}

.ListContent .image image {
  width: 126rpx;
  height: 126rpx;
  border-radius: 10rpx;
}

.tobe_aid {
  display: block;
}

.ListContent .image image {
  margin-right: 15rpx;
}

.ListContent .image image:nth-child(5n) {
  margin-right: 0;
}

.ListContent .money {
  text-align: right;
  font-size: 28rpx;
  color: #000;
  line-height: 80rpx;
  padding-right: 20rpx;
  height: 40rpx;
}

.describe {
  display: flex;
  flex-direction: column;
  height: 126rpx;
  width: 74%;
}

.describe .title {
  display: block;
  font-size: 30rpx;
  color: rgb(51, 51, 51);
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.describe .price {
  color: #a31e13;
  font-size: 30rpx;
  font-weight: bold;
}

.describe .specifications {
  display: flex;
}

.describe .money {
  display: flex;
  align-items: flex-end;
  color: rgb(185, 24, 50);
  font-size: 30rpx;
}

.button {
  width: 93%;
  margin: auto;
  display: flex;
  justify-content: flex-end;
  padding-top: 20rpx;
}

.button view {
  width: 180rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: rgba(255, 96, 50, 1);
  border-radius: 28rpx;
  /* border:1px solid #f4aa11; */
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  margin-left: 2%;
}

.button view.pay {
  /* border:none; */
  background-color: rgba(255, 96, 50, 1);
  color: #fff;
}

.button .LogisticsInfo {
  font-size: 30rpx;
  color: #ff6032;
  border: none;
}

.commodity_screenshowModal {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_boxshowModal {
  width: 100%;
  overflow: hidden;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 2000;
  padding-top: 20rpx;
  height: 100%;
}

.popup {
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: center; */
  margin-top: 40%;
  height: 100%;
  /* position: relative */
}

.close {
  width: 62rpx;
  height: 62rpx;
  margin-top: 400rpx;
  /* margin-top:70rpx */
}

.popup .popupInfo {
  height: 440rpx;
  width: 550rpx;
  background-color: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.img {
  width: 234rpx;
  height: 236rpx;
  margin-top: -583rpx;
  /* position: absolute;
  top:125rpx;
  left:273rpx; */
}

.popup .popupInfo .info {
  display: flex;
  flex-direction: column;
  margin-left: 5%;
  line-height: 40rpx;
  margin-top: 138rpx;
  width: 95%;
  justify-content: flex-start;
}

.popup .popupInfo .DuplicateNum {
  margin: auto;
  width: 230rpx;
  height: 68rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 68rpx;
  text-align: center;
  background-color: #ff6032;
}

.delete {
  width: 31rpx;
  height: 31rpx;
  margin-left: 20rpx;
}

.deletePopu {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  color: #000;
  font-size: 36rpx;
  border-radius: 25rpx;
  position: fixed;
  top: 25%;
  z-index: 1000;
  left: 7.5%;
  width: 85%;
}

.deletePopu .info {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 206rpx;
  border-bottom: 1px solid rgba(210, 211, 213, 1);
}

.deletePopu .info .title {
  font-size: 36rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(0, 0, 0, 1);
  margin-bottom: 32rpx;
}

.deletePopu .info .tips {
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  margin-bottom: 50rpx
}

.deletePopu .Inquiry {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
}

.deletePopu .Inquiry .cancel {
  width: 190rpx;
  height: 55rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(230, 0, 18, 1);
  border-radius: 28px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(241, 32, 12, 1);
  margin-right: 35rpx;
}

.deletePopu .Inquiry .confirm {
  width: 190rpx;
  height: 55rpx;
  background: rgba(230, 0, 18, 1);
  border-radius: 28rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
